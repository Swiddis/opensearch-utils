source = big5
| where `agent.type` = 'filebeat'
| where `@timestamp` >= '2023-01-01 00:00:00' and `@timestamp` <= '2023-01-02 00:00:00'
| stats count(), count(agent.name) by `agent.type`, `host.name`, `cloud.region`
| fields + `agent.type`, `host.name`, `cloud.region`, `count()`, `count(agent.name)`
